  "tokenColors": [
    /* comment. */
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#606974a1",
        "fontStyle": "italic"
      }
    },
    /* constant. */
    {
      "scope": ["constant"],
      "settings": {
        "foreground": "#2491a9"
      }
    },
    {
      "scope": ["constant.other"],
      "settings": {
        "foreground": "#fe896c"
      }
    },
    {
      "scope": ["constant.numeric", "constant.charcter.escape"],
      "settings": {
        "foreground": "#fe896c"
      }
    },
    {
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#f6c177"
      }
    },
    {
      "scope": ["constant.language.import-export-all"],
      "settings": {
        "foreground": "#fd75b5"
      }
    },
    /* entity. */
    {
      "scope": ["entity", "entity.name", "entity.name.function.member"],
      "settings": {
        "foreground": "#9dd0d9"
      }
    },
    {
      "scope": ["entity.name.constant", "variable.other.constant"],
      "settings": {
        "foreground": "#fd75b5"
      }
    },
    {
